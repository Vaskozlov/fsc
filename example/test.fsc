class S
{
    public var signature = 0

    public def __init__()
    {
        print("S has been constructed\n");
    }

    public def __del__()
    {
        print("S has been deleted")
    }

    public def test()
    {
        return 42
    }
}

def main()
{
    var a : S = S()
    var s = a.signature
    var t = a.test()

    print("{} {}\n", s, t);
    return 0
}
