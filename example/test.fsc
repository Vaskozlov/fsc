class S
{
    public var someVariable : i32

    public func __init__()
    {
        someVariable = 42
    }

    public func __add__(in S other)
    {
        var tmp = this
        tmp.someVariable = tmp.someVariable + other.someVariable
        return tmp
    }
}

var foo = S().someVariable
let pi = 3.141592653589793F64

func f(in i32 i) -> i32
{
    if i > 2
    {
        return i
    }

    return i + f(i + 1)
}

func main()
{
    var x = true
    var s = S()

    print("{} {}\n", f(0), "1013".toI32())
    return 0
